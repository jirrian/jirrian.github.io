	"person-thing-1":
		{
			"name": "Person Thing #1",
			"year": {
				"start": 2019,
				"end": 2019
			},
			"description": {
				"medium": "Custom printed circuit boards and lazercut acrylic",
				"concept": "Part one of ITP thesis where the artist is examining the blurry line between “person” and “thing” by positioning herself as a cyborg making cyborgs. Person Thing #1 is a garment constructed of acrylic and printed circuit boards or pcbs with speakers and colored leds. The speakers are fed audio through aux cord inputs connected to mobile devices playing different videos. The speakers’ audio blend together, conceptually imbuing the skin with media."
			},
			"mediums":[
				"wearable-tech",
				"installation"
			],
